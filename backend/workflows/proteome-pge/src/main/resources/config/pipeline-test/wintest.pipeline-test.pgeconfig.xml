<?xml version="1.0" encoding="UTF-8"?>
<pgeConfig>
   <!-- NOTE: This PGE in configured to run on a Windows machine -->
   <!-- How to run the PGE -->
   <exe dir="[CygwinJobDir]" shell="C:\cygwin\Cygwin.bat">
      <!-- cd to PGE root -->
      <cmd>date &gt; [CygwinJobDir]/date.output</cmd>
   </exe>
   <!-- Custom metadata to add to output files -->
   <customMetadata>
      <metadata key="WinWorkflowMetKey" val="WinWorkflowMetVal" workflowMet="true"/>
   </customMetadata>
</pgeConfig>
